<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.open9527.code.lib">


    <!--INTERNET-->
    <uses-permission android:name="android.permission.INTERNET" />

    <!--读取,写入文件-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <!--CONTACTS-->
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />


    <application
        android:name=".Application"
        android:allowBackup="true"
        android:icon="@mipmap/launch"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/launch"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".module.splash.SplashActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="SplashActivity"
            android:launchMode="singleTask"
            android:screenOrientation="user"
            android:theme="@style/SplashTheme"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="MainActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".module.other.OtherTitleActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="OtherActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".module.image.compression.CompressImageTitleActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="CompressImageActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".module.image.load.ImageLoadTitleActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="ImageLoadActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".module.image.preview.PreviewTitleActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="PreviewActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".module.browser.BrowserTitleActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="BrowserActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".module.statelayout.StateTitleActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="StateActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".module.bglib.BackgroundLibTitleActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="BackgroundLibActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".module.customview.TabLayoutTitleActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="TabLayoutActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".module.customview.CoordinatorLayoutTitleActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="CoordinatorLayoutActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".module.other.RecycleViewActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="RecycleViewActivity"
            android:launchMode="singleTop" />


    </application>


</manifest>
